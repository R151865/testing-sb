#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/home/workspace/sample-project/server/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/bin/is-base64/node_modules:/home/workspace/sample-project/server/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/bin/node_modules:/home/workspace/sample-project/server/node_modules/.pnpm/is-base64@1.1.0/node_modules/is-base64/node_modules:/home/workspace/sample-project/server/node_modules/.pnpm/is-base64@1.1.0/node_modules:/home/workspace/sample-project/server/node_modules/.pnpm/node_modules:/home/workspace/sample-project/server/node_modules:/home/workspace/sample-project/node_modules:/home/workspace/node_modules:/home/node_modules:/node_modules:/home/workspace/sample-project/server/node_modules/.pnpm/pusher@5.1.0-beta/node_modules/pusher/node_modules:/home/workspace/sample-project/server/node_modules/.pnpm/pusher@5.1.0-beta/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../is-base64@1.1.0/node_modules/is-base64/bin/is-base64" "$@"
else 
  exec node  "$basedir/../../../../../is-base64@1.1.0/node_modules/is-base64/bin/is-base64" "$@"
fi
